F90      = CONFIGURE_F90
CC       = CONFIGURE_CC
CPP      = /lib/cpp

LINUX_OBJ  = TIMEF.o
BYTE_ORDER = UPPLITTLEENDIAN
SIZE_I8R8  = -fdefault-integer-8 -fdefault-real-8
SIZE_I4R8  = -fdefault-real-8
